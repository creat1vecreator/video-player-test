  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.5",
    "@types/file-saver": "^2.0.5",
    "@types/jszip": "^3.4.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "axios": "^1.4.0",
    "classnames": "^2.3.2",
    "file-saver": "^2.0.5",
    "jszip": "^3.10.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-media-recorder": "^1.6.6",
    "react-redux": "^8.1.0",
    "react-router": "^6.13.0",
    "react-router-dom": "^6.13.0",
    "react-scripts": "5.0.1",
    "sass": "^1.63.4",
    "screen-recorder-react": "^0.0.6",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "start": "GENERATE_SOURCEMAP=false craco start",
    "build": "craco build",
    "clean": "rm -rf ./node_modules ./yarn-lock.json ./yarn.lock ./package-lock.json",
    "lint:fix": "yarn run lint --fix",
    "prettier": "prettier src --check",
    "prettier:fix": "yarn run prettier --write",
    "format": "yarn run prettier:fix && yarn run lint:fix",
    "reinstall": "yarn run clean && yarn run uncache && yarn install",
    "uncache": "yarn cache clean"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "craco": "^0.0.3",
    "craco-alias": "^3.0.1",
    "craco-sass-resources-loader": "^1.1.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "lint": "^0.8.19",
    "lint-fix": "^0.24.0",
    "prettier": "^2.8.8"
  }
